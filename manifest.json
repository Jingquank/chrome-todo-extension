{
    "manifest_version": 3,
    "name": "Startab",
    "description": "A beautiful and minimalist new tab page with a digital clock and to-do list",
    "version": "1.0",
    "permissions": [
      "storage"
    ],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "author": "Your Name",
    "homepage_url": "https://github.com/Jingquank/chrome-todo-extension"
  }